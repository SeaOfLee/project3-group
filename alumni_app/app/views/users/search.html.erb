<tbody>
    <% @users.each do |u| %>
      <tr>
        <td><%= u.first_name %> <%= u.last_name %></td>
        <td><%= u.email %></td>
        <td><%= u.is_admin %></td>
        <td><%= u.postal_code %></td>
        <td><%= u.country %></td>
        <td><%= u.program_attended %></td>
        <td><%= u.campus %></td>
        <td><%= u.campus %></td>
        <td><%= u.cohort %></td>
        <td><%= u.bio %></td>
        <td><%= link_to 'View ', u %>|</td>
        <td><%= link_to ' Edit ', edit_user_path(u) %> |</td>
        <td><%= link_to ' Delete', u, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>