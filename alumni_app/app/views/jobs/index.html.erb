<h1>Jobs#index</h1>
<p>Find me in app/views/jobs/index.html.erb</p>

<%= link_to "New Job", new_job_path %>

<% @jobs.each do |job| %>
  <h3><%= link_to job.job_title, job %></h3>
  <p><%= job.company %>, <%= job.location %></p>
  <p>Posted <%= time_ago_in_words(job.created_at) %> ago by User</p>
  <hr />
<% end %>

<%= will_paginate @jobs %>
